{
  "name": "burning-hear-graphql-server",
  "version": "1.0.0",
  "description": "A backend server refactor for Burning Heart",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect src/index.js",
    "test": "cross-env DB_ENV=testing jest --watch --verbose",
    "playground": "graphql playground --dotenv",
    "deploy": "prisma deploy --env-file variables.env"
  },
  "keywords": [
    "GraphQL",
    "Burning",
    "Heart",
    "GraphQL-Server",
    "Prisma"
  ],
  "author": "Nathan Thomas",
  "license": "MIT",
  "repository": {
    "url": "nathanthomas.dev"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "add": "^2.0.6",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.14.0",
    "cookie-parser": "^1.4.4",
    "datauri": "^1.1.0",
    "dotenv": "^7.0.0",
    "faker": "^4.1.0",
    "graphql": "^14.2.1",
    "graphql-cli": "^3.0.11",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.29.2",
    "prisma-binding": "^2.3.10",
    "stripe": "^6.28.0",
    "yarn": "^1.15.2"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2"
  }
}
